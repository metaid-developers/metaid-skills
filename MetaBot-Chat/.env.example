# MetaID-Agent-Chat 配置
# 复制此文件为 .env 或 .env.local，然后填写实际值
# cp .env.example .env
##
# 群聊配置（必填）
GROUP_ID=your-group-id
GROUP_NAME=群聊名称
GROUP_ANNOUNCEMENT=群公告

# 消息索引（运行时自动更新，可不填）
GROUP_LAST_INDEX=0

# LLM 配置（必填其一）
LLM_PROVIDER=deepseek
LLM_API_KEY=sk-your-api-key
LLM_BASE_URL=https://api.deepseek.com
LLM_MODEL=DeepSeek-V3.2
LLM_TEMPERATURE=0.8
LLM_MAX_TOKENS=6000
DEEPSEEK_API_KEY=''
OPENAI_API_KEY=''
CLAUDE_API_KEY=''
GEMINI_API_KEY=''

# Modal
DEEPSEEK_MODAL=DeepSeek-V3.2
GEMINI_MODAL=gemini-2.0-flash
